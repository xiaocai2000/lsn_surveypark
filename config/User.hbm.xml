<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.jizhuomi.surveypark.model.User" table="users">
		<id name="id" column="id" type="integer">
			<generator class="identity"></generator>
		</id>
		<property name="email" column="email" type="string" length="50"></properties>
		<property name="password" column="password" type="string" length="50"></properties>
		<property name="nickName" column="nickname" type="string" length="50"></properties>
		<property name="regDate" column="regdate" type="timestamp" update="false"></properties>
	</class>
</hibernate-mapping>